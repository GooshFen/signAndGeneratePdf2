{% extends 'base.html.twig' %}

{% block title %}TeletravailForm index{% endblock %}

{% block body %}
    <h1>TeletravailForm index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NatureContrat</th>
                <th>Quotite</th>
                <th>ConnexionInternet</th>
                <th>AttestationHonneur</th>
                <th>AttestationAssurance</th>
                <th>ActiviteEligible</th>
                <th>PeriodeEssaiEnCours</th>
                <th>AutonomieSuffisante</th>
                <th>ConditionsEligibilites</th>
                <th>ConditionsTechMatAdm</th>
                <th>AvisSupHierarchique</th>
                <th>CommentaireSupHierarchique</th>
                <th>AvisDrh</th>
                <th>CommentaireDrh</th>
                <th>SignatureSupHierarchique</th>
                <th>SignatureDrh</th>
                <th>SignatureCollab</th>
                <th>JourneeTeletravaillees</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for teletravail_form in teletravail_forms %}
            <tr>
                <td>{{ teletravail_form.id }}</td>
                <td>{{ teletravail_form.natureContrat }}</td>
                <td>{{ teletravail_form.quotite }}</td>
                <td>{{ teletravail_form.connexionInternet ? 'Yes' : 'No' }}</td>
                <td>{{ teletravail_form.attestationHonneur }}</td>
                <td>{{ teletravail_form.attestationAssurance }}</td>
                <td>{{ teletravail_form.activiteEligible ? 'Yes' : 'No' }}</td>
                <td>{{ teletravail_form.periodeEssaiEnCours ? 'Yes' : 'No' }}</td>
                <td>{{ teletravail_form.autonomieSuffisante ? 'Yes' : 'No' }}</td>
                <td>{{ teletravail_form.conditionsEligibilites ? 'Yes' : 'No' }}</td>
                <td>{{ teletravail_form.conditionsTechMatAdm ? 'Yes' : 'No' }}</td>
                <td>{{ teletravail_form.avisSupHierarchique ? 'Yes' : 'No' }}</td>
                <td>{{ teletravail_form.commentaireSupHierarchique }}</td>
                <td>{{ teletravail_form.avisDrh ? 'Yes' : 'No' }}</td>
                <td>{{ teletravail_form.commentaireDrh }}</td>
                <td>{{ teletravail_form.signatureSupHierarchique }}</td>
                <td>{{ teletravail_form.signatureDrh }}</td>
                <td>{{ teletravail_form.signatureCollab }}</td>
                <td>{{ teletravail_form.journeeTeletravaillees }}</td>
                <td>
                    <a href="{{ path('app_teletravail_form_show', {'id': teletravail_form.id}) }}">show</a>
                    <a href="{{ path('app_teletravail_form_edit', {'id': teletravail_form.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="20">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_teletravail_form_new') }}">Create new</a>
{% endblock %}
